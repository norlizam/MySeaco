<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:id="@+id/scrollViewTableLayoutDetail"
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:fillViewport="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <TableLayout
            android:id="@+id/tableLayoutDetail"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/image"
            android:background="#800000"
            android:orientation="vertical">

            <TableRow
                android:id="@+id/tableRow1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="2dip"
                android:background="#ffffff">

                <TextView
                    android:id="@+id/mapsOffDetail_id_title_dialog"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dip"
                    android:text="@string/mapsOffDetail_id_title_dialog"
                    android:textSize="12sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/mapsOffDetail_id_title2_dialog"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="10dip"
                    android:text="@string/doubleDot"
                    android:textSize="10sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/mapsOffDetail_id_dialog"
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:padding="5dip"
                    android:textSize="12sp" />
            </TableRow>

            <TableRow
                android:id="@+id/tableRow2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="1dip"
                android:background="#ffffff">

                <TextView
                    android:id="@+id/mapsOffDetail_barcode_title_dialog"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dip"
                    android:text="@string/mapsOffDetail_barcode_title_dialog"
                    android:textSize="12sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/mapsOffDetail_barcode_title2_dialog"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="10dip"
                    android:text="@string/doubleDot"
                    android:textSize="10sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/mapsOffDetail_barcode_dialog"
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:padding="10dip"
                    android:textSize="12sp" />
            </TableRow>

            <TableRow
                android:id="@+id/tableRow9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="1dip"
                android:background="#ffffff">

                <TextView
                    android:id="@+id/mapsOffDetail_latlon_title_dialog"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dip"
                    android:text="@string/mapsOffDetail_latlon_title_dialog"
                    android:textSize="12sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/mapsOffDetail_latlon_title2_dialog"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="10dip"
                    android:text="@string/doubleDot"
                    android:textSize="10sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/mapsOffDetail_latlon_dialog"
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:padding="10dip"
                    android:textSize="12sp" />
            </TableRow>

            <TableRow
                android:id="@+id/tableRow8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="1dip"
                android:background="#ffffff">

                <TextView
                    android:id="@+id/mapsOffDetail_address_title_dialog"
                    android:layout_width="20dp"
                    android:layout_height="wrap_content"
                    android:padding="5dip"
                    android:text="@string/mapsOffDetail_address_title_dialog"
                    android:textSize="12sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/mapsOffDetail_address_title2_dialog"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="10dip"
                    android:text="@string/doubleDot"
                    android:textSize="12sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/mapsOffDetail_address_dialog"
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:padding="10dip"
                    android:textSize="12sp" />
            </TableRow>

            <TableRow
                android:id="@+id/tableRow3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="1dip"
                android:background="#ffffff">

                <TextView
                    android:id="@+id/mapsOffDetail_address2_title_dialog"
                    android:layout_width="50dp"
                    android:layout_height="wrap_content"
                    android:padding="5dip"
                    android:text="@string/mapsOffDetail_address2_title_dialog"
                    android:textSize="12sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/mapsOffDetail_address2_title2_dialog"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="10dip"
                    android:text="@string/doubleDot"
                    android:textSize="10sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/mapsOffDetail_address2_dialog"
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:padding="10dip"
                    android:textSize="12sp" />
            </TableRow>

            <TableRow
                android:id="@+id/tableRow4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="1dip"
                android:background="#ffffff">

                <TextView
                    android:id="@+id/mapsOffDetail_createdDate_title_dialog"
                    android:layout_width="50dp"
                    android:layout_height="wrap_content"
                    android:padding="5dip"
                    android:text="@string/mapsOffDetail_createdDate_title_dialog"
                    android:textSize="12sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/mapsOffDetail_createdDate_title2_dialog"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="10dip"
                    android:text="@string/doubleDot"
                    android:textSize="10sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/mapsOffDetail_createdDate_dialog"
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:padding="10dip"
                    android:textSize="12sp" />
            </TableRow>

            <TableRow
                android:id="@+id/tableRow5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="1dip"
                android:background="#ffffff">

                <TextView
                    android:id="@+id/mapsOffDetail_createdBy_title_dialog"
                    android:layout_width="50dp"
                    android:layout_height="wrap_content"
                    android:padding="5dip"
                    android:text="@string/mapsOffDetail_createdBy_title_dialog"
                    android:textSize="12sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/mapsOffDetail_createdBy_title2_dialog"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="10dip"
                    android:text="@string/doubleDot"
                    android:textSize="10sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/mapsOffDetail_createdBy_dialog"
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:padding="10dip"
                    android:textSize="12sp" />
            </TableRow>
        </TableLayout>
    </ScrollView>

</RelativeLayout>