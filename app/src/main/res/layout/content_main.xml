<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="jasonpaulraj.myseaco.MainActivity"
    android:background="#ed5d5d">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_below="@drawable/logo1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:backgroundTint="#df6262"
        android:background="#f29191"
        android:id="@+id/linearLayout"></LinearLayout>

    <TextView
        android:text="Add Location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView5"
        android:layout_alignLeft="@+id/textView4"
        android:layout_alignStart="@+id/textView4"
        android:layout_below="@+id/imgBtnAddLoc" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/view_list"
        android:id="@+id/imgBtnViewList"
        android:layout_below="@+id/textView5"
        android:layout_alignLeft="@+id/imgBtnAddLoc"
        android:layout_alignStart="@+id/imgBtnAddLoc"
        android:layout_marginTop="27dp" />

    <TextView
        android:text="View Maps"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView4"
        android:layout_below="@+id/imgBtnViewLoc"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="View List Location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView8"
        android:layout_below="@+id/imgBtnViewList"
        android:layout_alignRight="@+id/imgBtnViewLoc"
        android:layout_alignEnd="@+id/imgBtnViewLoc" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/add_location1"
        android:id="@+id/imgBtnAddLoc"
        android:layout_marginTop="17dp"
        android:layout_below="@+id/textView4"
        android:layout_alignLeft="@+id/imgBtnViewLoc"
        android:layout_alignStart="@+id/imgBtnViewLoc" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/view_location1"
        android:id="@+id/imgBtnViewLoc"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

